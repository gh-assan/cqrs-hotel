{
  "name": "cqrs-hotel",
  "version": "1.0.0",
  "description": "Example application about CQRS and Event Sourcing",
  "directories": {
    "test": "src/test/js"
  },
  "dependencies": {
    "axios": "0.15.3",
    "date-fns": "1.28.5",
    "history": "4.6.0",
    "lodash-es": "4.17.4",
    "path-to-regexp": "1.7.0",
    "purecss": "0.6.2",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "redux": "3.6.0",
    "redux-form": "6.5.0",
    "redux-logger": "2.8.2",
    "uuid": "3.0.1"
  },
  "devDependencies": {
    "babel-core": "6.24.0",
    "babel-loader": "6.4.0",
    "babel-plugin-transform-async-to-generator": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-env": "1.6.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "chai": "3.5.0",
    "cross-env": "5.0.5",
    "css-loader": "0.27.3",
    "jsx-loader": "0.13.2",
    "mocha": "3.2.0",
    "source-map-explorer": "1.3.3",
    "style-loader": "0.14.0",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.2"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --compilers js:babel-core/register --require ./src/main/js/test_setup.js 'src/main/js/**/*.test.js'",
    "build": "cross-env NODE_ENV=production webpack --progress --colors -p",
    "autobuild": "webpack --progress --colors --watch",
    "start": "webpack-dev-server --content-base src/main/web --host 0.0.0.0 --progress --colors",
    "analyze": "source-map-explorer target/webpack/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orfjackal/cqrs-hotel.git"
  },
  "author": "Esko Luontola",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/orfjackal/cqrs-hotel/issues"
  },
  "homepage": "https://github.com/orfjackal/cqrs-hotel"
}
